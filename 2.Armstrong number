#include<stdio.h>
int main()
{
	int re,i,n,temp,sum=0;
	printf("enter number:");
	scanf("%d",&n);	
	while(temp>0)
	{
		re=temp%10;
		sum=sum+re*re*re;
		temp=temp/10;
	}
	if(sum==temp)
	{
		printf("%d is armstrong number",n);
	}
	else
	{
		printf("%d is not an armstrong number",n);
	}
	return 0;
}


Input:
Enter the armstrong number: 153
Output:
153 is a armstrong number
